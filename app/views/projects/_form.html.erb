<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :tags %>
    <%= f.input :feature_image_url %>
    <%= f.input :description %>
    <%= f.input :problem %>
    <%= f.input :solution %>
    <%= f.input :client %>
    <%= f.input :demo_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
